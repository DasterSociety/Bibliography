  author = {Lazarovits, James and Sindhwani, Shrey and Tavares, Anthony J. and Zhang, Yuwei and Song, Fayi and Audet, Julie and Krieger, Jonathan R. and Syed, Abdullah Muhammad and Stordy, Benjamin and Chan, Warren C.W.},
