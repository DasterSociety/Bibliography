  author = {Chen, Junyue and Oudeng, Gerile and Feng, Hongtao and Liu, Sixi and Li, Hung-Wing and Ho, Yi-Ping and Chen, Yan and Tan, Ying and Yang, Mo},
  date = {2022},
  journaltitle = {Small},
