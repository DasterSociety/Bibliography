@article{Harsha2019,
  title = {A Novel {{PEGylated}} Carbon Nanotube Conjugated Mangiferin: {{An}} Explorative Nanomedicine for Brain Cancer Cells},
  author = {Harsha, P. J. and Thotakura, Nagarani and Kumar, Manish and Sharma, Saurabh and Mittal, Anupama and Khurana, Rajneet Kaur and Singh, Bhupinder and Negi, Poonam and Raza, Kaisar},
  date = {2019},
  journaltitle = {Journal of Drug Delivery Science and Technology},
  volume = {53},
  issn = {17732247},
  doi = {10.1016/j.jddst.2019.101186},
  url = {https://doi.org/10.1016/j.jddst.2019.101186},
  abstract = {In the present study, polyethylene glycol-linked conjugate of carbon nanotubes with Mangiferin was synthesized and characterized by means of Fourier transform infrared and nuclear magnetic resonance spectroscopic techniques. Studies for the determination of particle size, polydispersity index and zeta potential were performed. Morphological characterization was performed using transmission electron microscopy. Phytochemical conjugated nanotubes were also evaluated for hemo-compatibility, protein binding capacity and in-vitro drug release. Cytotoxicity studies and flow cytometry were performed on U-87 cell lines. Drug release studies confirmed a spatiotemporal pattern of release at the cancer cell pH. Cytotoxicity studies proved that there was 1.28 folds decrease in the IC50 value indicating effective anticancer activity, whereas hemolytic profile established the safety. Flow cytometry confirmed effective induction of apoptosis with minimum necrosis by the nano-conjugate vis-\`a-vis the na\"ive drug. The pharmacokinetic study showcased that there was 4 times escalation in the area under the curve, i.e., bioavailability of the drug after the conjugation to that of plain mangiferin. From the obtained results, it can be concluded that these functionalized nanocarriers are capable of the effective and safer delivery of phytochemicals to the brain cancerous cells.},
  keywords = {Anti-cancer activity,Pharmacokinetics,Phytochemical,Protein binding,U-87 cells},
  file = {/Users/daster/Library/CloudStorage/OneDrive-InstitutoTecnologicoydeEstudiosSuperioresdeMonterrey/DNT/Zotero Attachments/Harsha et al_2019_A novel PEGylated carbon nanotube conjugated mangiferin.pdf}
}

@article{Hassoun2018,
  title = {A Droplet-Based Microfluidic Chip as a Platform for Leukemia Cell Lysate Identification Using Surface-Enhanced {{Raman}} Scattering},
  author = {Hassoun, Mohamed and R\"uger, Jan and Kirchberger-Tolstik, Tatiana and Schie, Iwan W. and Henkel, Thomas and Weber, Karina and Cialla-May, Dana and Krafft, Christoph and Popp, J\"urgen},
  date = {2018-01},
  journaltitle = {Analytical and Bioanalytical Chemistry},
  shortjournal = {Anal Bioanal Chem},
  volume = {410},
  number = {3},
  pages = {999--1006},
  issn = {1618-2642, 1618-2650},
  doi = {10.1007/s00216-017-0609-y},
  url = {http://link.springer.com/10.1007/s00216-017-0609-y},
  urldate = {2022-09-06},
  abstract = {A new approach is presented for cell lysate identification which uses SERS-active silver nanoparticles and a droplet-based microfluidic chip. Eighty-nanoliter droplets are generated by injecting silver nanoparticles, KCl as aggregation agent, and cell lysate containing cell constituents, such as nucleic acids, carbohydrates, metabolites, and proteins into a continuous flow of mineral oil. This platform enables accurate mixing of small volumes inside the meandering channels of the quartz chip and allows acquisition of thousands of SERS spectra with 785 nm excitation at an integration time of 1 s. Preparation of three batches of three leukemia cell lines demonstrated the experimental reproducibility. The main advantage of a high number of reproducible spectra is to apply statistics for large sample populations with robust classification results. A support vector machine with leave-one-batchout cross-validation classified SERS spectra with sensitivities, specificities, and accuracies better than 99\% to differentiate Jurkat, THP-1, and MONO-MAC-6 leukemia cell lysates. This approach is compared with previous published reports about Raman spectroscopy for leukemia detection, and an outlook is given for transfer to single cells.},
  langid = {english},
  keywords = {New,notion},
  file = {/Users/daster/Library/CloudStorage/OneDrive-InstitutoTecnologicoydeEstudiosSuperioresdeMonterrey/DNT/Zotero Attachments/Hassoun et al_2018_A droplet-based microfluidic chip as a platform for leukemia cell lysate.pdf}
}

@article{Haydon2015,
  title = {Nanoengineering: {{A Toolbox}} of {{Standards}} for {{Health}} and {{Safety}}},
  author = {Haydon, Brian},
  date = {2015},
  journaltitle = {Nanoengineering: Global Approaches to Health and Safety Issues},
  pages = {557--580},
  doi = {10.1016/B978-0-444-62747-6.00017-8},
  abstract = {Developing standards is an important means to provide guidance, best practices, and requirements for health and safety-related issues for nanotechnologies. With global cooperation of governments, industry, researchers, consumer interests, and others, collaborative work has produced a strong and growing foundation of standards from the International Organization for Standardization (ISO) and the International Electrotechnical Commission (IEC). A growing toolbox of standards now exists as a valuable resource for coming decades for use in research, health and safety, and commercialization for nanotechnologies. The nanotechnology practitioner is guided to useful standards that detail where we stand, what we know, and what can be applied, from a standards perspective for nanotechnologies. Topics include why standards for nanotechnologies matter, description of ISO/TC229 and IEC/TC113, the role of standards in regulations, and the ISO and IEC standards development process. Select ISO and IEC standards for nanotechnologies are described, with background on their development, including standards for terminology and nomenclature; measurement and characterization; health, safety, and environmental aspects of nanotechnologies; material specifications; performance assessment; and reliability. The application of these standards from one country's perspective (Canada) is described. As outlined, the development of standards for nanotechnologies continues concurrently with research and commercialization to advance responsible and sustainable global trade in nano-enhanced and nano-enabled products and systems. Standards play an important role as an evolving set of tools to be applied to support nanotechnologies.},
  keywords = {Health and safety,Measurand,Nano-enabled,Nano-enhanced,Nano-object,Nanotechnologies,Performance assessment,Regulations,Risk management,Standards,Terminology},
  file = {/Users/daster/Library/CloudStorage/OneDrive-InstitutoTecnologicoydeEstudiosSuperioresdeMonterrey/DNT/Zotero Attachments/Haydon_2015_Nanoengineering.pdf}
}

