  author = {L\'ibalov\'a, Helena and Costa, Pedro M. and Olsson, Magnus and Farcal, Lucian and Ortelli, Simona and Blosi, Magda and Topinka, Jan and Costa, Anna L. and Fadeel, Bengt},
