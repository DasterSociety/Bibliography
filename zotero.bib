@article{Deng2011,
  title = {Nanoparticle-Induced Unfolding of Fibrinogen Promotes {{Mac-1}} Receptor Activation and Inflammation},
  author = {Deng, Zhou J. and Liang, Mingtao and Monteiro, Michael and Toth, Istvan and Minchin, Rodney F.},
