@article{Flores2014,
  title = {Low Consumption Single-Use Microvalve for Microfluidic {{PCB-based}} Platforms},
  author = {Flores, G. and Aracil, C. and Perdigones, F. and Quero, J. M.},
  date = {2014},
  journaltitle = {Journal of Micromechanics and Microengineering},
  volume = {24},
  number = {6},
  issn = {13616439},
  doi = {10.1088/0960-1317/24/6/065013},
  abstract = {In this paper, a single-use and unidirectional microvalve with low consumption of energy for PCB-based microfluidic platforms is reported. Its activation is easy because it works as a fuse. The fabrication process of the device is based on PCB technology and a typical SU-8 process, using the PCB as a substrate and SU-8 for the microfluidic channels and chambers. The microvalve is intended to be used to impulse small volumes of fluids and it has been designed to be highly integrable in PCB-based microfluidic platforms. The proposed device has been fabricated, integrated and tested in a general purpose microfluidic circuit, resulting in a low activation time, of about 100 {$\mu$}s, and a low consumption of energy, with a maximum of 27 mJ. These results show a significant improvement because the energy consumption is about 84\% lower and the time response is about four orders of magnitude shorter if compared with similar microvalves for impulsion of fluids on PCB-based platforms. \textcopyright{} 2014 IOP Publishing Ltd.},
  keywords = {microvalve,PCB-MEMS,SU-8},
  file = {/Users/daster/Library/CloudStorage/OneDrive-InstitutoTecnologicoydeEstudiosSuperioresdeMonterrey/DNT/Zotero Attachments/Flores et al_2014_Low consumption single-use microvalve for microfluidic PCB-based platforms.pdf}
}

@article{Flores2018,
  title = {Lab-Protocol-on-{{PCB}}: {{Prototype}} of a Laboratory Protocol on Printed Circuit Board Using {{MEMS}} Technologies},
  author = {Flores, Guadalupe and Aracil, Carmen and Perdigones, Francisco and Quero, Jos\'e M.},
  date = {2018},
  journaltitle = {Microelectronic Engineering},
  volume = {200},
  pages = {26--31},
  issn = {01679317},
  doi = {10.1016/j.mee.2018.08.003},
  url = {https://linkinghub.elsevier.com/retrieve/pii/S0167931718303964},
  abstract = {A portable prototype of a lab protocol has been developed by the integration of electronics, control, and microfluidics. The main characteristics are ease of fabrication and simple use. The system is composed of a microfluidic platform, which is inserted into an electronic device. The microfluidic platform implements the transport of fluids, meanwhile the electronic module is on charge of the activation, heating and the interface with the user. The impulsion is based on the combination between pressurized chambers and the valves that control the pass of fluid. In particular, in this paper, an example of application of the lab protocol is reported. The experiments show the behaviour of the whole system under the required specifications. The proposed system is versatile and can be reconfigured for different applications. This system means a contribution towards the integration of protocols of laboratory on portable platforms.},
  keywords = {Lab protocol,PCB-MEMS},
  file = {/Users/daster/Library/CloudStorage/OneDrive-InstitutoTecnologicoydeEstudiosSuperioresdeMonterrey/DNT/Zotero Attachments/Flores et al_2018_Lab-protocol-on-PCB.pdf;/Users/daster/Library/CloudStorage/OneDrive-InstitutoTecnologicoydeEstudiosSuperioresdeMonterrey/DNT/Zotero Attachments/Flores et al_2018_Lab-protocol-on-PCB2.pdf}
}

@article{Fu2017,
  title = {Lab-on-{{PCB-based}} Micro-Cytometer for Circulating Tumor Cells Detection and Enumeration},
  author = {Fu, Yusheng and Yuan, Qingbo and Guo, Jinhong},
  date = {2017},
  journaltitle = {Microfluidics and Nanofluidics},
  volume = {21},
  number = {2},
  pages = {1--4},
  issn = {16134990},
  doi = {10.1007/s10404-017-1854-2},
  abstract = {Dietary indoles, present in Brassica plants such as cabbage, broccoli, and Brussels sprouts, have been shown to provide potential protection against hormone-dependent cancers. 3,3'-Diindolylmethane (DIM) is under study as one of the main protective indole metabolites. Postmenopausal women aged 50-70 yr from Marin County, California, with a history of early-stage breast cancer, were screened for interest and eligibility in this pilot study on the effect of absorbable DIM (BioResponse-DIM) supplements on urinary hormone metabolites. The treatment group received daily DIM (108 mg DIM/day) supplements for 30 days, and the control group received a placebo capsule daily for 30 days. Urinary metabolite analysis included 2-hydroxyestrone (2-OHE1), 16-alpha hydroxyestrone (16alpha-OHE1), DIM, estrone (El), estradiol(E2), estriol (E3), 6beta-hydroxycortisol (6beta-OHC), and cortisol in the first morning urine sample before intervention and 31 days after intervention. Nineteen women completed the study,for a total of 10 in the treatment group and 9 in the placebo group. DIM-treated subjects, relative to placebo, showed a significant increase in levels of2-OHE1 (P=0. 020), DIM (P =0. 045), and cortisol (P = 0.039), and a nonsignificant increase of 47\% in the 2-OHE1/16alpha-OHE1 ratio from 1.46 to 2.14 (P=0.059). In this pilot study, DIM increased the 2-hydroxylation of estrogen urinary metabolites.},
  keywords = {Circulating tumor cells (CTCs),Impedance,Lab-on-PCB,Microfluidic cytometer,Red blood cells,White blood cells},
  file = {/Users/daster/Zotero/storage/QUX2HRT5/Fu, Yuan, Guo - 2017 - Lab-on-PCB-based micro-cytometer for circulating tumor cells detection and enumeration.pdf}
}

